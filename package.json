{
  "version": "0.3.0",
  "name": "masa",
  "description": "small library to format date in Indonesian language",
  "author": "Armedi",
  "license": "MIT",
  "homepage": "https://github.com/armedi/masa",
  "keywords": [
    "date format",
    "time format",
    "format tanggal",
    "format waktu"
  ],
  "main": "dist/masa.js",
  "unpkg": "dist/masa.umd.js",
  "module": "dist/masa.m.js",
  "exports": "./dist/masa.modern.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/armedi/masa.git"
  },
  "bugs": {
    "url": "https://github.com/armedi/masa/issues"
  },
  "scripts": {
    "start": "tsnd src/index.ts",
    "build": "microbundle src/index.ts",
    "test": "jest --config jest.config.js --coverage",
    "lint": "eslint src --ext js,ts",
    "prepare": "husky install",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {},
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "always"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "microbundle": "^0.13.0",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "ts-node-dev": "^1.1.6",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  }
}
